                        PATH_ROOT=''
                      PATH_APACHE=$PATH_ROOT'/etc/apache2'
      PATH_APACHE_SITES_AVAILABLE=$PATH_APACHE'/sites-available'
             PATH_APACHE_INCLUDES=$PATH_APACHE'/includes'
