# some path variables dehydrated
      FOLDER_CONFIG_DEHYDRATED='/etc/dehydrated'
             FOLDER_DEHYDRATED='/opt/dehydrated'
         FOLDER_WWW_DEHYDRATED='/var/www/dehydrated'
            SYMLINK_DEHYDRATED='/usr/local/sbin/dehydrated'
                  DOMAINS_FILE="$FOLDER_CONFIG_DEHYDRATED/domains.txt"
	
        FILE_CONFIG_DEHYDRATED="$FOLDER_CONFIG_DEHYDRATED/config"
           FILE_SRC_DEHYDRATED="$FOLDER_DEHYDRATED/dehydrated"
FILE_EXAMPLE_CONFIG_DEHYDRATED="$FOLDER_DEHYDRATED/docs/examples/config"
 
 
 
